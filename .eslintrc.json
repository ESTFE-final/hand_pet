{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module",
		"requireConfigFile": false
	},
	"plugins": ["react"],
	"rules": {},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
